<style>
  .count{
      color: black;
      font-size: 13px;
      font-weight: bold;
  }
</style>
<script type="text/javascript" src="/assets/showSubMenu.js"></script>

<div id="default" style="width: 100%; height: 400px; text-align: center; vertical-align: top;">

    <div class="button child_menu printed_cases  " onclick="window.location = '/view_printed_cases'" tag=0 title="View Complete Cases">
        <table style="text-align: left;">
            <tr>
                <td rowspan="2">
                  <img src="<%= @icoFolder %>" height="60"/>  <br />
                  <span class="count"><%= (@stats['DC-PRINTED'] + @stats["HQ-PRINTED"])%></span>
                </td>
                <td style="height: 30px;">
                  All Printed Cases
                </td>
            </tr>
            <tr>
                <td style="font-size: 11px; font-style: italic;">
                    View Cases Printed at HQ and DC
                </td>
            </tr>
        </table>
    </div>


    <div class="button child_menu manage_cases  " onclick="window.location = '/view_printed_cases?loc=hq'" tag=0 title="View Cases">
      <table style="text-align: left;">
        <tr>
          <td rowspan="2">
            <img src="<%= @icoFolder %>" height="60"/>  <br />
            <span class="count"><%= @stats['HQ-PRINTED']%></span>
          </td>
          <td style="height: 30px;">
            Printed at HQ
          </td>
        </tr>
        <tr>
          <td style="font-size: 11px; font-style: italic;">
            View Cases Printed at HQ
          </td>
        </tr>
      </table>
    </div>


    <div class="button child_menu manage_cases  " onclick="window.location = '/view_printed_cases?loc=dc'" tag=0 title="View Cases">
      <table style="text-align: left;">
        <tr>
          <td rowspan="2">
            <img src="<%= @icoFolder %>" height="60"/>  <br />
              <span class="count"><%= @stats['DC-PRINTED']%></span>
          </td>
          <td style="height: 30px;">
            Printed at DC
          </td>
        </tr>
        <tr>
          <td style="font-size: 11px; font-style: italic;">
            View Cases Printed at DC
          </td>
        </tr>
      </table>
    </div>
</div>
